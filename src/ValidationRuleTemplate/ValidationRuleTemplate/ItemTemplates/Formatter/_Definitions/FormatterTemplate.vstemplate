<?xml version="1.0" encoding="utf-8"?>
<VSTemplate Version="3.0.0" xmlns="http://schemas.microsoft.com/developer/vstemplate/2005" Type="Item">
  <TemplateData>
    <DefaultName>Formatter.cs</DefaultName>
    <Name>Formatter Template</Name>
    <Description>Creates an formatter Rule that derives from IValueFormatter for Formatting your properties.</Description>
    <ProjectType>CSharp</ProjectType>
    <AppliesTo>CSharp + MultiTarget</AppliesTo>
    <Icon>FormatterIcon.png</Icon>
    <TemplateGroupID>CodeSharing-Native</TemplateGroupID>
    <TemplateID>Microsoft.CSharp.Rule</TemplateID>
    <SortOrder>10</SortOrder>
